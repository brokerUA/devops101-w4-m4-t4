            <script type="text/javascript" src="/js/qrcode.min.js"></script>
                <script type="text/javascript">
            var qrcode = null;

            function printQr() {
                var link = document.getElementById("pickfiles").getAttribute('href');
                if( link.indexOf("http") !== 0 ) {
                    link = 'https:' + link;
                }
                if (!QRCode) {
                    return
                }
                if (qrcode == null) {
                    qrcode = new QRCode(document.getElementById("qrcode"), {
                        text: link,
                        width: 250,
                        height: 250,
                        //colorDark: "#E5322D",
                        colorDark: "#383E45",
                        colorLight: "#ffffff",
                        correctLevel: QRCode.CorrectLevel.M
                    });
                } else {
                    qrcode.makeCode(link);
                }
            }

            function waitForLink() {
                var link = document.getElementById("pickfiles").getAttribute('href');
                if (/javascript/.test(link)) {
                    setTimeout(function () {
                        waitForLink();
                    }, 200)
                } else {
                    printQr();
                }
            }

            waitForLink();
        </script>
